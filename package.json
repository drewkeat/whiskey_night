{
  "name": "whiskey_night",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "clear-cache": "rm -rf .next/cache prisma/client",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate-local": "dotenv -e .env.local -- supabase migration up",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset && supabase storage --local --experimental cp -r ./supabase/seed/whiskey-pics ss:///imgs",
    "scrape-seed": "dotenv -e .env.local -- node supabase/seed/scrape-seed.js",
    "scrape-seed-by-name": "dotenv -e .env.local -- node supabase/seed/scrape-seed-by-name.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.16.1",
    "@mui/material": "^5.16.0",
    "@mui/material-nextjs": "^5.16.4",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.44.3",
    "axios": "^1.7.3",
    "dotenv-cli": "^7.4.2",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "^5"
  }
}
