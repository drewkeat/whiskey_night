{
  "name": "whiskey_night",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clear-cache": "rm -rf .next/cache prisma/client",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pmigrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:dev": "dotenv -e .env.local -- supabase migration up",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.16.1",
    "@mui/material": "^5.16.0",
    "@mui/material-nextjs": "^5.16.4",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.44.3",
    "dotenv-cli": "^7.4.2",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "typescript": "^5"
  }
}
